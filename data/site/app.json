{
    "msn": {
        "preview": "https://github.com/CircuitalMinds/static/raw/main/apps/vahngraff/img/preview.jpg",
        "scripts": ["/assets/js/utils.js"],
        "styles": []
    },
    "spectrum": {
        "preview": "https://github.com/CircuitalMinds/static/raw/main/apps/vahngraff/img/preview.jpg",
        "scripts": ["/assets/js/utils.js"],
        "styles": []
    },
    "drive": {
        "file": {
            "icon": "icon mif-file-empty fg-teal",
            "views": {
                "data": {"view": "#data-view", "types": ["json", "yml", "txt"]},
                "videos": {"view": "#videos-view", "types": ["mp4", "mp3", "wmv", "mkv"]},
                "pictures": {"view": "#pictures-view", "types": ["jpg", "png", "svg", "gif", "jpeg"]},
                "scripts": {"view": "#scripts-view", "types": ["py", "js", "css", "sqlite3", "", "sh"]},
                "documents": {"view": "#documents-view", "types": ["html", "pdf", "md", "markdown", "ipynb"]}
            }
        },
        "folder": {
            "icon": "icon mif-folder fg-teal",
            "path": "https://circuitalminds.github.io/static/storage",
            "url_data": "https://circuitalminds.github.io/static/data/storage.json",
            "directories": ["pictures", "videos", "documents", "data", "scripts"],
            "total_size": 0,
            "content": []
        },
        "preview": "https://github.com/CircuitalMinds/static/raw/main/apps/vahngraff/img/preview.jpg",
        "scripts": ["/assets/js/utils.js"],
        "styles": []
    },
    "vahngraff": {
        "buttons": [
            {
                "icon": "mif-volume-minus",
                "id": "volume-minus",
                "onclick": "Player.Vol.setMinus();"
            },
            {
                "icon": "mif-volume-plus",
                "id": "volume-plus",
                "onclick": "Player.Vol.setPlus();"
            },
            {
                "icon": "mif-previous",
                "id": "previous",
                "onclick": "Player.Previous();"
            },
            {
                "icon": "mif-play",
                "id": "play",
                "onclick": "$('#video-player')[0].play();"
            },
            {
                "icon": "mif-pause",
                "id": "pause",
                "onclick": "$('#video-player')[0].pause();"
            },
            {
                "icon": "mif-next",
                "id": "next",
                "onclick": "Player.Next();"
            }
        ],
        "player": {
            "icons": {
                "loop": "mif-loop2",
                "play": "mif-play",
                "stop": "mif-stop",
                "volume-high": "mif-volume-low",
                "volume-low": "mif-volume-low",
                "volume-medium": "mif-volume-medium",
                "volume-mute": "mif-volume-mute2"
            },
            "id": "video-player",
            "poster": "https://github.com/CircuitalMinds/static/raw/main/apps/vahngraff/img/poster.gif",
            "url_data": "https://circuitalminds.github.io/static/data/videos/metadata.json"
        },
        "search": {
            "id": "results",
            "header": "<h2>Search Results</h2>",
            "body": "<ul id='jkl-results' class='feed-list bg-darkTeal fg-light'></ul>",
            "footer": "Don't Stop The Classics",
            "btn_id": "query"
        },
        "preview": "https://github.com/CircuitalMinds/static/raw/main/apps/vahngraff/img/preview.jpg",
        "scripts": ["/assets/js/utils.js", "/assets/js/search.js"],
        "styles": ["/assets/css/modal.css"]
    }
}